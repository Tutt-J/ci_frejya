{% extends 'base.html.twig' %}

{% block title %}Home{% endblock %}

{% block body %}
    {% if app.user %}
        <h1>Vous êtes connecté</h1>
        <a href="{{ path("app_logout")}}">Se déconnecter</a>
    {% else %}
        <h1>Vous n’êtes pas connecté</h1>
        <a href="{{ path("app_login")}}">Se connecter</a>
    {% endif %}
{% endblock %}
